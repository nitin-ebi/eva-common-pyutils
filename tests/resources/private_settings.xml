<settings>
    <profiles>
        <profile>
            <id>dummy</id>
            <properties>
                <eva.mongo.host>mongos-host1.example.com:27017,mongos-host2.example.com:27017</eva.mongo.host>
                <eva.mongo.user>mongouser</eva.mongo.user>
                <eva.mongo.passwd>mongopassword</eva.mongo.passwd>
                <eva.mongo.passwd.url-encoded>password</eva.mongo.passwd.url-encoded>
                <eva.mongo.auth.db>admin</eva.mongo.auth.db>
                <eva.mongo.read-preference>secondaryPreferred</eva.mongo.read-preference>
                <eva.mongo.collections.variants>variants_2_0</eva.mongo.collections.variants>
                <eva.mongo.collections.files>files_2_0</eva.mongo.collections.files>
                <eva.mongo.collections.annotation-metadata>annotationMetadata_2_0</eva.mongo.collections.annotation-metadata>
                <eva.mongo.collections.features>features_2_0</eva.mongo.collections.features>
                <eva.mongo.collections.annotations>annotations_2_0</eva.mongo.collections.annotations>
                <eva.version>2.0</eva.version>
                <eva.context.root>http://www.ebi.ac.uk/eva</eva.context.root>
                <eva.count-stats.url>https://www.ebi.ac.uk/eva/webservices/count-stats</eva.count-stats.url>
                <eva.count-stats.username>statsuser</eva.count-stats.username>
                <eva.count-stats.password>statspassword</eva.count-stats.password>
                <eva.evapro.datasource>evapro</eva.evapro.datasource>
                <eva.evapro.user>metauser</eva.evapro.user>
                <eva.evapro.password>metapassword</eva.evapro.password>
                <eva.evapro.jdbc.url>jdbc:postgresql://host1.example.com:5432/database</eva.evapro.jdbc.url>
                <eva.evapro.eva-stats.schema>eva_stats</eva.evapro.eva-stats.schema>
                <eva.accession.mongo.database>eva_accession_sharded</eva.accession.mongo.database>
                <eva.accession.mongo.human.database>eva_accession_human_sharded</eva.accession.mongo.human.database>
                <eva.accession.jdbc.url>jdbc:postgresql://host1.example.com:5432/accjtdb</eva.accession.jdbc.url>
                <eva.accession.user>accuser</eva.accession.user>
                <eva.accession.password>accpassword</eva.accession.password>
                <eva.variant.jdbc.url>jdbc:postgresql://host1.example.com:5432/jtdb</eva.variant.jdbc.url>
                <eva.variant.user>varuser</eva.variant.user>
                <eva.variant.password>varpassword</eva.variant.password>
                <contig-alias.admin-user>aliasuser</contig-alias.admin-user>
                <contig-alias.admin-password>aliaspassword</contig-alias.admin-password>
                <contig-alias.url>https://www.ebi.ac.uk/eva/webservices/contig-alias</contig-alias.url>
            </properties>
        </profile>
    </profiles>
</settings>